{
  "ts-node": {
    "files": true
  },
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "typeRoots": ["./src/types"],
    "paths": {
      "*": ["./node_modules/*"]
    }
  },
  "include": [
    "./src/**/*",]
  ,
  "exclude": ["./node_modules"]
}
